<!DOCTYPE html>
{{ partial "head.html" . }}
<body>
    {{ partial "header.html" . }}
    <main class="content" role="main">
        <div class="container">
            {{ $paginator := .Paginate (where .Site.RegularPages "Type" "posts") }}
            {{ range .Paginator.Pages }}
                {{ .Render "li" }}
            {{ end }}
            {{ partial "pagination.html" . }}
        </div>
    </main>
    {{ partial "footer.html" . }}
</body>
</html>